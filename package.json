{
  "name": "lvskripts",
  "displayName": "lvskripts",
  "description": "Provides snippets, syntax highlighting and other features for LVskripts programming language.",
  "version": "1.0.0",
  "icon": "./icons/icon.png",
  "publisher": "Dzintars",
  "repository": {
    "type": "git",
    "url": "https://github.com/superkaplis/LVskripts-extension"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "lvskripts",
        "aliases": [
          "LVskripts",
          "lvskripts"
        ],
        "extensions": [
          ".lv"
        ],
        "icon": {
          "light": "./icons/fileIcon.png",
          "dark": "./icons/fileIcon.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "lvskripts",
        "path": "./snippets/lvskripts-snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "lvskripts",
        "scopeName": "source.lv",
        "path": "./syntaxes/lvskripts.tmLanguage.json"
      }
    ]
  }
}